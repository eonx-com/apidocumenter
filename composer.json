{
  "name": "loyaltycorp/apidocumenter",
  "description": "Library for generation of OpenAPI v3 documentation",
  "keywords": [
    "loyaltycorp",
    "apidoc",
    "openapi"
  ],
  "license": "BSD-3-Clause",
  "type": "library",
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "cebe/php-openapi": "^1.2",
    "doctrine/orm": "^2.6",
    "eoneopay/utils": "^0.2.10",
    "phpdocumentor/reflection-docblock": "^4.3",
    "symfony/property-info": "^4.3",
    "symfony/serializer": "^4.3"
  },
  "require-dev": {
    "eoneopay/standards": "dev-master",
    "friendsofphp/php-cs-fixer": "^2.9",
    "laravel/lumen-framework": "^6.1",
    "nikic/fast-route": "^1.3",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^8.0",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "3.*",
    "symfony/property-access": "^4.3"
  },
  "suggest": {
    "laravel/lumen-framework": "Required when using apidocumenter with Lumen",
    "nikic/fast-route": "Required when using apidocumenter with Lumen"
  },
  "autoload": {
    "files": [
      "src/SchemaBuilders/buildReference.php"
    ],
    "psr-4": {
      "LoyaltyCorp\\ApiDocumenter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\LoyaltyCorp\\ApiDocumenter\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
